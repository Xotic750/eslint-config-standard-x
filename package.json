{
	"name": "@Xotic750/eslint-config-standard-x",
	"version": "1.0.0",
	"description": "My shareable eslint config for standard modules.",
	"homepage": "https://github.com/Xotic750/module-bolierplate-x",
	"author": {
		"name": "Graham Fairweather",
		"email": "xotic750@gmail.com"
	},
	"copyright": "Copyright (c) 2017",
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"find-unused": "eslint-find-rules --unused",
		"prepublish": "! type safe-publish-latest >/dev/null 2>&1 || safe-publish-latest && in-install || (npm test && npm run find-unused)",
		"test": "npm run --silent tests-only",
		"posttest": "npm run --silent security",
		"tests-only": "eslint index.js",
		"security": "nsp check",
    "precommit": "npm test && npm posttest",
    "prepush": " npm test && npm posttest"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/xotic750/eslint-config-standard-x.git"
	},
	"bugs": {
		"url": "https://github.com/Xotic750/module-bolierplate-x/issues"
	},
	"keywords": [
		"eslint",
		"eslint-config",
		"standard"
	],
	"dependencies": {},
	"devDependencies": {
		"@ljharb/eslint-config": "^11.0.0",
		"eslint": "^3.19.0",
		"eslint-find-rules": "^1.14.3",
		"eslint-plugin-compat": "^1.0.2",
		"eslint-plugin-css-modules": "^2.7.1",
		"eslint-plugin-eslint-comments": "^1.0.0",
		"eslint-plugin-jsdoc": "^3.0.2",
		"eslint-plugin-json": "^1.2.0",
		"eslint-plugin-no-use-extend-native": "^0.3.12",
		"husky": "^0.13.3",
		"nsp": "^2.6.3",
		"safe-publish-latest": "^1.1.1"
	},
	"peerDependencies": {
		"eslint": "^3.19.0",
		"eslint-plugin-compat": "^1.0.2",
		"eslint-plugin-css-modules": "^2.7.1",
		"eslint-plugin-eslint-comments": "^1.0.0",
		"eslint-plugin-jsdoc": "^3.0.2",
		"eslint-plugin-json": "^1.2.0",
		"eslint-plugin-no-use-extend-native": "^0.3.12"
	},
	"engines": {
		"node": ">= 4"
	},
	"publishConfig": {
		"access": "public"
	}
}
